# Add these setting to .bash_profile or .bashrc
alias tmux="TERM=screen=256color-bce tmux"

PATH=$HOME/CMake/bin:$HOME/python/bin:${PATH}
C_INCLUDE_PATH=$HOME/Python/include/python3.11
export C_INCLUDE_PATH

# If the proginal vim is support python3, this setting is not necessry.
PATH=$HOME/vim/bin:${PATH}
